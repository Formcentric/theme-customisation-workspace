{
    "name": "theme-customisation-workspace",
    "private": true,
    "version": "1.1.0-SNAPSHOT",
    "type": "module",
    "scripts": {
        "storybook": "storybook dev -p 6006",
        "dev-cloud": "pnpm fc-utils-themes list && vite build && VITE_FC_ENV=cloud vite",
        "dev-local": "pnpm fc-utils-themes list && vite build && VITE_FC_ENV=local vite",
        "create-theme": "pnpm fc-utils-themes create",
        "update-theme": "pnpm fc-utils-themes update",
        "reset": "pnpm fc-utils-themes reset",
        "css": "sh -c 'pnpm exec sass --no-source-map \"$0\" && pnpm exec postcss \"$0\"/**/*.css --config postcss.config.js --no-map --replace'",
        "build": "vite build"
    },
    "dependencies": {
        "@formcentric/client": "3.6.1",
        "@formcentric/utils": "0.0.1",
        "@fortawesome/fontawesome-svg-core": "^6.7.2",
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@fortawesome/react-fontawesome": "^0.2.6",
        "@headlessui/react": "^2.2.9",
        "htm": "^3.1.1",
        "lodash": "^4.17.21",
        "motion": "^11.18.2",
        "preact": "^10.27.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "zustand": "^5.0.8"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^3.2.7",
        "@eslint/js": "^9.39.1",
        "@storybook/addon-docs": "^8.6.14",
        "@storybook/addon-essentials": "^8.6.14",
        "@storybook/addon-links": "^8.6.14",
        "@storybook/blocks": "^8.6.14",
        "@storybook/manager-api": "^8.6.14",
        "@storybook/react": "^8.6.14",
        "@storybook/react-vite": "^8.6.14",
        "@storybook/test": "^8.6.14",
        "@storybook/theming": "^8.6.14",
        "@types/lodash": "^4.17.20",
        "@types/react": "^18.3.26",
        "@types/react-dom": "^19.2.2",
        "@vitejs/plugin-react": "^4.7.0",
        "autoprefixer": "^10.4.21",
        "cssnano": "^7.1.2",
        "eslint": "^9.39.1",
        "eslint-plugin-storybook": "^0.12.0",
        "glob": "^11.0.3",
        "globals": "^15.15.0",
        "http-proxy-middleware": "^3.0.5",
        "path": "^0.12.7",
        "postcss": "^8.5.6",
        "postcss-cli": "^11.0.1",
        "postcss-import": "^16.1.1",
        "postcss-url": "^10.1.3",
        "prettier": "^3.6.2",
        "sass": "^1.93.3",
        "storybook": "^8.6.14",
        "styled-components": "^6.1.19",
        "typescript": "~5.6.3",
        "typescript-eslint": "^8.46.3",
        "vite": "^6.4.1"
    },
    "eslintConfig": {
        "extends": [
            "plugin:storybook/recommended"
        ]
    },
    "packageManager": "pnpm@10.19.0"
}
