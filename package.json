{
    "name": "theme-customization-workspace",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev-docs": "storybook dev -p 6006",
        "dev-cloud": "pnpm tsx ./cli/scripts/generateThemeList.ts && vite build && VITE_FC_ENV=cloud vite",
        "dev-local": "pnpm tsx ./cli/scripts/generateThemeList.ts && vite build && VITE_FC_ENV=local vite",
        "create-theme": "pnpm tsx ./cli/scripts/create.ts",
        "update-theme": "pnpm tsx ./cli/scripts/update.ts",
        "reset": "pnpm tsx ./cli/scripts/reset.ts",
        "css": "sh -c 'pnpm exec sass --no-source-map \"$0\" && pnpm exec postcss \"$0\"/**/*.css --config postcss.config.js --no-map --replace'",
        "build": "vite build"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^3.2.4",
        "@eslint/js": "^9.14.0",
        "@formcentric/client": "3.2.2",
        "@headlessui/react": "^2.2.0",
        "@storybook/addon-docs": "^8.5.8",
        "@storybook/addon-essentials": "^8.5.8",
        "@storybook/addon-links": "^8.5.8",
        "@storybook/blocks": "^8.5.8",
        "@storybook/manager-api": "^8.5.8",
        "@storybook/react": "^8.5.8",
        "@storybook/react-vite": "^8.5.8",
        "@storybook/test": "^8.5.8",
        "@storybook/theming": "^8.5.8",
        "@types/fs-extra": "^11.0.4",
        "@types/lodash": "^4.17.13",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^19.0.1",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "chalk": "^5.4.1",
        "chokidar": "^4.0.1",
        "cssnano": "^7.0.6",
        "eslint": "^9.14.0",
        "eslint-plugin-storybook": "^0.11.3",
        "fs-extra": "^11.2.0",
        "glob": "^11.0.0",
        "globals": "^15.12.0",
        "http-proxy-middleware": "^3.0.3",
        "inquirer": "^12.4.2",
        "lodash": "^4.17.21",
        "path": "^0.12.7",
        "postcss": "^8.4.49",
        "postcss-cli": "^11.0.0",
        "postcss-import": "^16.1.0",
        "postcss-url": "^10.1.3",
        "prettier": "^3.4.2",
        "rollup-plugin-copy": "^3.5.0",
        "sass": "^1.72.0",
        "storybook": "^8.5.8",
        "styled-components": "^6.1.13",
        "tsx": "^4.19.2",
        "typescript": "~5.6.3",
        "typescript-eslint": "^8.13.0",
        "vite": "^5.4.10"
    },
    "dependencies": {
        "motion": "^11.13.5",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "zustand": "^5.0.1"
    },
    "eslintConfig": {
        "extends": [
            "plugin:storybook/recommended"
        ]
    }
}
