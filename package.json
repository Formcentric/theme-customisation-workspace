{
  "name": "theme-customisation-workspace",
  "private": true,
  "version": "1.0.2-SNAPSHOT",
  "type": "module",
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "dev-cloud": "pnpm fc-utils-themes list && vite build && VITE_FC_ENV=cloud vite",
    "dev-local": "pnpm fc-utils-themes list && vite build && VITE_FC_ENV=local vite",
    "create-theme": "pnpm fc-utils-themes create",
    "update-theme": "pnpm fc-utils-themes update",
    "reset": "pnpm fc-utils-themes reset",
    "css": "sh -c 'pnpm exec sass --no-source-map \"$0\" && pnpm exec postcss \"$0\"/**/*.css --config postcss.config.js --no-map --replace'",
    "build": "vite build"
  },
  "dependencies": {
    "@formcentric/client": "3.4.0",
    "@formcentric/utils": "0.0.1",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@headlessui/react": "^2.2.0",
    "htm": "^3.1.1",
    "lodash": "^4.17.21",
    "motion": "^11.13.5",
    "preact": "^10.26.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/js": "^9.14.0",
    "@storybook/addon-docs": "^8.6.11",
    "@storybook/addon-essentials": "^8.6.11",
    "@storybook/addon-links": "^8.6.11",
    "@storybook/blocks": "^8.6.11",
    "@storybook/manager-api": "^8.6.11",
    "@storybook/react": "^8.6.11",
    "@storybook/react-vite": "^8.6.11",
    "@storybook/test": "^8.6.11",
    "@storybook/theming": "^8.6.11",
    "@types/lodash": "^4.17.13",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^9.14.0",
    "eslint-plugin-storybook": "^0.12.0",
    "glob": "^11.0.0",
    "globals": "^15.12.0",
    "http-proxy-middleware": "^3.0.3",
    "path": "^0.12.7",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-url": "^10.1.3",
    "prettier": "^3.4.2",
    "sass": "^1.72.0",
    "storybook": "^8.6.11",
    "styled-components": "^6.1.15",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "packageManager": "pnpm@9.15.3+sha512.1f79bc245a66eb0b07c5d4d83131240774642caaa86ef7d0434ab47c0d16f66b04e21e0c086eb61e62c77efc4d7f7ec071afad3796af64892fae66509173893a"
}
